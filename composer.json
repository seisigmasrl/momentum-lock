{
  "name": "seisigmasrl/momentum-lock",
  "description": "A Laravel package that lets you handle Laravel authorizations on the frontend level.",
  "version": "1.0.0",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Ricardo Vargas",
      "email": "rvargas@seisigma.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Momentum\\Lock\\": "src/",
      "Momentum\\Lock\\Tests\\": "tests/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Momentum\\Lock\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10|^11|^12",
    "illuminate/auth": "^10|^11|^12",
    "spatie/laravel-data": "^4.0",
    "spatie/laravel-typescript-transformer": "^2.4"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0|^9.0|^10.0",
    "larastan/larastan": "^1.0|^2.0|^3.0",
    "laravel/pint": "^1.2",
    "pestphp/pest": "^1.22|^2.0|^3.0",
    "pestphp/pest-plugin-laravel": "^1.3|^2.0|^3.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Momentum\\Lock\\LockServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
